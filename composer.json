{
  "name": "crusherrl/ics-link-generator",
  "type": "project",
  "description": "A small vanilla php ics link generator",
  "keywords": [
    "ics",
    "ics link",
    "link generator",
    "ics generator"
  ],
  "require": {
    "php": "^8.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.25"
  },
  "scripts": {
    "php-fixer": [
      "vendor/bin/php-cs-fixer --config=./src/PHPConfigLoader.php fix src "
    ]
  }
}
